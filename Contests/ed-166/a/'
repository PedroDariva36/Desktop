#include <algorithm>
#include <bits/stdc++.h>
#include <vector>
#define ll long long
#define endl '\n'
#define all(x) (x).begin(), (x).end()
#define rall(x) (x).rbegin(), (x).rend()
#define pb push_back

using namespace std;

int main (int argc, char *argv[]){
    cin.tie(0)->sync_with_stdio(0);
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        vector<char> a; 
        for (auto &i: a) {
            cin >> i; 
        }
        sort(a.begin(),a.end());
        for (auto i: a) {
            cout << i ; 
        }
        cout << endl;
    }

    return 0;
}
